<Window x:Class="FFmpegView.WpfDemo2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ffmpeg="clr-namespace:FFmpegView.Wpf;assembly=FFmpegView.Wpf"
        mc:Ignorable="d" Title="MainWindow" Height="450" Width="800">
    <TabControl SelectionChanged="TabControl_SelectionChanged">
        <TabItem Header="FFmpegView">
            <ffmpeg:FFmpegView x:Name="playerView" />
        </TabItem>
        <TabItem Header="FFmpegVisualView">
            <ffmpeg:FFmpegVisualView x:Name="playerVView" />
        </TabItem>
    </TabControl>
</Window>